func inverse(lst){
    var new_lst = []
    var length = len(lst) - 1
    for i = length to -1 step -1:
        var aux = get(lst, i)
        append(new_lst, aux)
    end
    return new_lst
}

func sumList(lst){
    var length = len(lst)
    var sum = 0
    for i = 0 to length:
        var aux = get(lst, i)
        var sum = sum + aux
    end
    return sum
}

var lst = [1, 2, 3, 4, 5]

var new_lst = inverse(lst)

print(new_lst)

var sum = sumList(lst)

print(sum)
